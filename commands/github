const Discord = require ("discord.js")

exports.run = async (message, args) => {
message.delete()

const embed = new Discord.MessageEmbed()
.setColor('#DCDCDC')
.setTitle('Repositório GitHub')
.setDescription('Caso você queira acesso ao código fonte da Maya, você pode encontrá-lo facilmente no GitHub')
.addFields(
{
name: 'Código no Github',
value: `[Clique aqui](${https://github.com/rodycouto/Maya-GitHub.git})`
},
{
name: 'Programador',
value: 'Rody#3756'
}
)
.setFooter('Apoiem os programadores')

message.channel.send(embed).then(m => m.delete({timeout: 8000}))
}
